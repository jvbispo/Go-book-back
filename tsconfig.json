{
  "compilerOptions": {
    "target": "es5",
    "module": "commonjs",
    "outDir": "./dist",
    "rootDir": "./src",
    "strict": true,
    "strictPropertyInitialization": false,
    "baseUrl": "./src",
    "allowJs": true,
    "esModuleInterop": true ,
     "experimentalDecorators": true,
     "emitDecoratorMetadata": true,
    "forceConsistentCasingInFileNames": true,
    "resolveJsonModule": true
  },
  "exclude": [
    "jest.config.ts",
    "jest.config.js",
    "prettier.config.js",
    "node_modules",
    "coverage",
    "tmp"
  ],

}
